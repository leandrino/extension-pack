{
	"name": "aura-extension-pack",
	"displayName": "aura-extension-pack",
	"description": "A collection of extension that we use at Aura project.",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Extension Packs"
	],
	"publisher": "leandrino",
	"keywords": [
	  "lint",
	  "prettier",
	  "tag"
	],
	"galleryBanner": {
		"color": "#030518",
		"theme": "dark"
	},
	"activationEvents": [
		"onCommand:aura-extension-pack.helloWorld"
	],
	"main": "./out/extension.js",
	"repository": {
		"url": "https://github.com/leandrino/extension-pack"
	},
	"homepage": "https://github.com/leandrino/extension-pack",
	"bugs": {
		"url": "https://github.com/leandrino/extension-pack/issues"
	  },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"extensionPack": [
		"formulahendry.auto-rename-tag",
		"editorconfig.editorconfig",
		"dbaeumer.vscode-eslint",
		"ms-vsliveshare.vsliveshare",
		"esbenp.prettier-vscode"
	]
}
